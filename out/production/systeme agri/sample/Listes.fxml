<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.ListesController">
   <children>
      <ImageView fitHeight="400.0" fitWidth="600.0">
         <image>
            <Image url="@infoperso.jpg" />
         </image>
      </ImageView>
      <Button fx:id="listesCand" layoutX="95.0" layoutY="187.0" mnemonicParsing="false" onAction="#liste" prefHeight="55.0" prefWidth="424.0" text="LISTE DES CANDIDATS" />
      <Button fx:id="ajouterCand" layoutX="95.0" layoutY="121.0" mnemonicParsing="false" onAction="#ajouter" prefHeight="55.0" prefWidth="424.0" text="AJOUTER UN CANDIDAT" />
      <Button fx:id="quitter" layoutX="183.0" layoutY="330.0" mnemonicParsing="false" onAction="#quitter" prefHeight="25.0" prefWidth="235.0" text="QUITTER" />
      <Label layoutX="240.0" layoutY="-58.0" text="CHOIX DE LA LISTE" textFill="#f8f3f3" />
   </children>
</AnchorPane>
